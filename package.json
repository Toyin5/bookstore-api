{
  "name": "bookstore",
  "version": "0.1.0",
  "description": "bookstore",
  "main": "src/server.ts",
  "engines": {
    "node": ">=18.x"
  },
  "scripts": {
    "start": "npm run build && node build/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "rimraf ./build && tsc",
    "sequelize:create": "npx sequelize db:create",
    "generate:migration": "npx sequelize-cli migration:generate --name",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "drop": "npx sequelize db:drop"
  },
  "author": "Toyin5",
  "license": "AGPL-3.0",
  "private": true,
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.7.2",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "ts-custom-error": "^3.3.1",
    "tsyringe": "^4.10.0",
    "winston": "^3.17.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.5.2",
    "rimraf": "^6.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
}
